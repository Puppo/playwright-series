{
  "name": "react-e2e",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "e2e": "playwright test",
    "e2e:prd": "playwright test -c playwright.config.prd.ts",
    "e2e:report": "playwright show-report",
