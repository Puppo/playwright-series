    "react-dom-confetti": "^0.2.0",
    "zustand": "^4.1.5"
  },
  "devDependencies": {
    "@playwright/experimental-ct-react": "^1.32.1",
    "@playwright/test": "^1.32.1",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^3.1.0",
    "json-server": "^0.17.1",
    "json-server-auth": "^2.1.0",
    "lighthouse": "^10.1.0",
    "sass": "^1.56.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1"
  }
}